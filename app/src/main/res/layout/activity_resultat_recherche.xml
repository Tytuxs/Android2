<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ResultatRecherche">

    <TextView
        android:id="@+id/textViewResultatRecherche"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Chambres disponibles:"
        tools:layout_editor_absoluteX="15dp"
        tools:layout_editor_absoluteY="20dp"
        tools:ignore="MissingConstraints" />

    <ListView
        android:id="@+id/listViewListeChambre"
        android:layout_width="375dp"
        android:layout_height="413dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/buttonReserver"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="reserver"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/listViewListeChambre" />


</androidx.constraintlayout.widget.ConstraintLayout>